<Application x:Class="WpfArranger.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfArranger"
             xmlns:view="clr-namespace:WpfArranger.View"
             xmlns:vm="clr-namespace:WpfArranger.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:DocumentsViewModel}">
            <view:DocView/>
        </DataTemplate>
         
    </Application.Resources>
</Application>
